<fieldset id='reviewformheadcoachtab' class='hidden'>

  <legend>
    <div class="col-md-4">
      <img alt="Coach Icon" class="top-icon" src="<%= asset_path( 'coach_icon.png' ) %>">
      <h1>Head Coach</h1>
    </div>
  </legend>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Player Relationships</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Player Development</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Recruiting</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Professional Connections</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Sport Knowledge</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- radio group -->
<div class="form-group">
  <label class="col-md-6 control-label" for="radios">Would You Play for this Coach Again?</label>
  <div class="col-md-3">
    <label class="radio-inline" for="radios-0">
      <input type="radio" name="radios" id="radios-0" value="false" checked="checked">
      Yes
    </label>
    <label class="radio-inline" for="radios-1">
      <input type="radio" name="radios" id="radios-1" value="true">
      No
    </label>
  </div>
</div>


  <!-- Textarea -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="textarea">Additional Comments About the Head Coach</label>
    <div class="col-md-3">
      <textarea class="form-control" id="textarea" name="textarea"></textarea>
    </div>
  </div>

  <!-- Button -->
  <div class="form-group">
    <label class="col-md-4 control-label" for="singlebutton"></label>
    <div class="col-md-1">
      <button id="reviewformbacktogetstarted" name="singlebutton" class="btn btn-primary">Program</button>
    </div>
    <div class="col-md-1">
      <button id="reviewformfacilities" name="singlebutton" class="btn btn-primary">Facilities</button>
    </div>
  </div>

</fieldset>