<fieldset id='reviewformlocationtab' class='hidden'>

  <legend>
    <div class="col-md-4">
      <img alt="Location Icon" class="top-icon" src="<%= asset_path( 'location_icon.png' ) %>">
      <h1>Location</h1>
    </div>
  </legend>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Nightlife</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Community Interest</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Program Tradition</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Select Basic -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="selectbasic">Waether</label>
    <div class="col-md-3">
      <%= render partial: 'stars_div' %>
    </div>
  </div>

  <!-- Textarea -->
  <div class="form-group">
    <label class="col-md-6 control-label" for="textarea">Additional Comments About the Location</label>
    <div class="col-md-3">
      <textarea class="form-control" id="textarea" name="textarea"></textarea>
    </div>
  </div>

  <!-- Button -->
  <div class="form-group">
    <label class="col-md-4 control-label" for="singlebutton"></label>
    <div class="col-md-1">
      <button id="reviewformbacktofacilities" name="singlebutton" class="btn btn-primary">Facilities</button>
    </div>
    <div class="col-md-1">
      <button id="reviewformeducation" name="singlebutton" class="btn btn-primary">Education</button>
    </div>
  </div>

</fieldset>